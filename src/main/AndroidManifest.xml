<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.spar">

    <uses-sdk android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:versionCode="1"
        android:versionName="1.0"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".button.AisleDetails"
            android:label="Code Scanned"
        />
        <activity
            android:name=".button.Aisle"
            android:label="Code Scanned"
        />
        <activity
            android:name=".button.Activity2"
            android:label="Code Scanning"
        />
        <activity
            android:name=".swipe.Single"
            android:label="Code Scanned"
        />
        <activity
            android:name=".swipe.Intersection"
            android:label="Code Scanned"
        />
        <activity
            android:name=".swipe.Scan"
            android:label="Code Scanning"
        />
        <activity
            android:name=".MainActivity"
            android:label="Sparrow">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>